{
    "name": "jasny/controller",
    "description": "A general controller for PSR-7",
    "keywords": ["MVC", "controller"],
    "license": "MIT",
    "authors": [
        {
            "name": "Arnold Daniels",
            "email": "arnold@jasny.net",
            "homepage": "http://www.jasny.net"
        }
    ],
    "support": {
        "issues": "https://github.com/jasny/controller/issues",
        "source": "https://github.com/jasny/controller"
    },
    "require": {
        "php": ">=8.0.0",
        "psr/http-message": "^1.0",
        "willdurand/negotiation": "^2.2",
        "ralouphie/mimey": "^1.0",
        "psr/http-server-middleware": "^1.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "jasny/php-code-quality": "^2.7",
        "slim/slim": "4.*",
        "slim/psr7": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "Jasny\\Controller\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jasny\\Test\\Controller\\": "tests/"
        }
    }
}
